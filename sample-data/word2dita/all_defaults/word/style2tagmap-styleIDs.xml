      level="1"/>
    <style
      styleId="ListNumber"
      containerType="ol"
      tagName="li"
      level="1"
      structureType="block"
      topicZone="body"/>
    <style
      styleId="ListNumber2"
      containerType="ol"
      tagName="li"
      level="2"
      structureType="block"
      topicZone="body"/>
    
    
<style
  styleId="DefTerm"
  structureType="dt"
  tagName="dt"
  dlEntryType="dlentry"
  containerType="dl"
  topicZone="body"
/>
<style
  styleId="DefDesc"
  structureType="dd"
  tagName="dd"
  dlEntryType="dlentry"
  containerType="dl"
  topicZone="body"
/>
    
<style 
  styleId="MySectionTitle"
  structureType="section" 
  tagName="title"
  sectionType="section"
  topicZone="body"
  useAsTitle="yes"  
/>
    
<style 
  styleId="LessonTitle"
  structureType="topicTitle"
  initialSectionType="lcIntro"
  topicType="learningContent"
  bodyType="learningContentbody"
  level="1"
/>    
    
<style styleId="section"
  structureType="section"
  tagName="p"
  topicZone="body"
  spectitle="#toColon"
  
/>
    
    
  </styles>
</style2tagmap>